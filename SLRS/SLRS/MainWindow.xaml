<Window x:Class="SLRS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select" Height="293" Width="444"
        ResizeMode="NoResize">
    <Grid>
        <Button Name="btn_evaluate" Content="Evaluate Output File" Click="btn_eval_click"  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,20,0,0" Width="180" Height="60"/>
        <Button Name="btn_kinect" Content="Start Kinect"    Click="btn_start_click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="240,20,0,0" Width="180" Height="60"/>
        <Button Name="btn_LRA" Content="LRA"                Click="btn_lra"  HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,100,0,0" Width="180" Height="60" IsEnabled="False" />
        <Button Name="btn_graph" Content="Errorgraph"   Click="btn_graph_click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="240,100,0,0" Width="180" Height="60" />
        <Button Name="btn_pcl" Content="Evaluate PointCloud"   Click="btn_pcl_click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,180,0,0" Width="180" Height="60" />
    </Grid>
</Window>